      title: {
            display: true,
            text: 'Franklin County Traffic Violation Count by Sex, Jan. 2nd 2018 - April 19th, 2018'
        },
      legend: {
            display: true
         },
      responsive: true
    }
});

var ctx = document.getElementById("trafRace").getContext('2d');

var myChart = new Chart(ctx, {
    type: 'doughnut',
