        fill: false
      }
    ]
  },
  options: {
    title: {
      display: true,
      text: 'Franklin County Violations Count by Hour, Jan. 2nd 2018 - April 19th, 2018'
    },
    scales: {
        yAxes: [{
          scaleLabel: {
    display: true,
    labelString: 'Violation Count'
  },
            ticks: {
                beginAtZero:true
            }
        }],
        xAxes: [{
          scaleLabel: {
